<?xml version="1.0" encoding="utf-8"?>
<!-- AppOne Application Import Schema File -->
<!-- Contains the schema definitions for the XML file submitted to the AppOne Connect ImportApplication() web service method -->
<!-- Modified by Balaji on 12/08/2016 for Applicant Other Income Source-->
<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="AppOneApplication" nillable="false">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Version" type="XMLVersion" minOccurs="1" maxOccurs="1" nillable="false" />

        <xs:element name="Sender" minOccurs="1" maxOccurs="1">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="UserName" minOccurs="1" maxOccurs="1" nillable="false">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:minLength value="4" />
                    <xs:maxLength value="25" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="Password" minOccurs="1" maxOccurs="1" nillable="false">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:minLength value="8" />
                    <xs:maxLength value="25" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="RecordID" minOccurs="1" maxOccurs="1" nillable="false">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:minLength value="1" />
                    <xs:maxLength value="20" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>

        <xs:element name="Dealer">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="SenderID" minOccurs="0" maxOccurs="1" nillable="true">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:minLength value="0" />
                    <xs:maxLength value="20" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="AppOneID" type="xs:unsignedInt" minOccurs="0" maxOccurs="1" nillable="false" />
              <xs:element name="Type" type="DealerType" minOccurs="1" maxOccurs="1" nillable="false" />
              <xs:element name="Name" minOccurs="1" maxOccurs="1" nillable="false">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:minLength value="5" />
                    <xs:maxLength value="50" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="Address1" minOccurs="1" maxOccurs="1" nillable="false">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:minLength value="5" />
                    <xs:maxLength value="50" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="Address2" minOccurs="0" maxOccurs="1" nillable="true">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:minLength value="0" />
                    <xs:maxLength value="50" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="City" type="City" minOccurs="1" maxOccurs="1" nillable="false" />
              <xs:element name="County" type="County" minOccurs="0" maxOccurs="1" nillable="true" />
              <xs:element name="State" type="State" minOccurs="1" maxOccurs="1" nillable="false" />
              <xs:element name="ZipCode" type="ZipCode" minOccurs="1" maxOccurs="1" nillable="false" />
              <xs:element name="Phone" type="Phone" minOccurs="1" maxOccurs="1" nillable="false" />
              <xs:element name="Fax" type="Phone" minOccurs="0" maxOccurs="1" nillable="false" />
              <!-- Added by Param on 6/5/2012 - Name of Sales Person -->
              <xs:element name="SalesPersonName" minOccurs="0" maxOccurs="1" nillable="true">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:minLength value="0" />
                    <xs:maxLength value="50" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <!-- Added by Param on 6/5/2012 - Name of F&I Manager -->
              <xs:element name="FIPersonName" minOccurs="0" maxOccurs="1" nillable="true">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:minLength value="0" />
                    <xs:maxLength value="50" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <!-- Added by Param on 8/12/2016 -->
        <xs:element name="JointCreditAcknowledgment" type="xs:boolean" minOccurs="0" maxOccurs="1" nillable="false" />
        
        <xs:element name="Borrowers" minOccurs="1" maxOccurs="1">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Borrower" minOccurs="1" maxOccurs="2">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="NumOrder" type="xs:unsignedByte" minOccurs="1" maxOccurs="1" nillable="false" />
                    <!-- Added by Param on 9/5/2013 -->
                    <xs:element name="Type" type="BorrowerType" minOccurs="1" maxOccurs="1" nillable="false" />
                    <xs:element name="RelationshipToPrimaryBorrower" type="ApplicantRelation" minOccurs="0" maxOccurs="1" nillable="false" />                    
                    <!-- Modified by Param on 9/5/2013 -->
                    <xs:element name="RelationshipDescription" minOccurs="0" maxOccurs="1" nillable="false">
                      <xs:simpleType>
                        <xs:restriction base="xs:string">
                          <xs:minLength value="0" />
                          <xs:maxLength value="100" />
                        </xs:restriction>
                      </xs:simpleType>
                    </xs:element>
                    <xs:element name="FirstName" minOccurs="0" maxOccurs="1" nillable="true">
                      <xs:simpleType>
                        <xs:restriction base="xs:string">
                          <xs:minLength value="0" />
                          <xs:maxLength value="20" />
                        </xs:restriction>
                      </xs:simpleType>
                    </xs:element>
                    <xs:element name="MiddleName" minOccurs="0" maxOccurs="1" nillable="true">
                      <xs:simpleType>
                        <xs:restriction base="xs:string">
                          <xs:minLength value="0" />
                          <xs:maxLength value="20" />
                        </xs:restriction>
                      </xs:simpleType>
                    </xs:element>
                    <!-- Modified by Param on 9/5/2013 -->
                    <xs:element name="LastName" minOccurs="0" maxOccurs="1" nillable="true">
                      <xs:simpleType>
                        <xs:restriction base="xs:string">
                          <xs:minLength value="0" />
                          <xs:maxLength value="20" />
                        </xs:restriction>
                      </xs:simpleType>
                    </xs:element>

                    <xs:element name="Suffix" type="NameSuffix" minOccurs="0" maxOccurs="1" nillable="false" />
                    <xs:element name="SSN" type="SSN" minOccurs="0" maxOccurs="1" nillable="false" />
                    <xs:element name="DOB" type="xs:date" minOccurs="0" maxOccurs="1" nillable="false" />
                    <xs:element name="HomePhone" type="Phone" minOccurs="0" maxOccurs="1" nillable="false" />
                    <xs:element name="MobilePhone" type="Phone" minOccurs="0" maxOccurs="1" nillable="false" />
                    <xs:element name="DLNo" minOccurs="0" maxOccurs="1" nillable="true">
                      <xs:simpleType>
                        <xs:restriction base="xs:string">
                          <xs:minLength value="0" />
                          <xs:maxLength value="15" />
                        </xs:restriction>
                      </xs:simpleType>
                    </xs:element>
                    <xs:element name="DLState" type="State" minOccurs="0" maxOccurs="1" nillable="false" />
                    <xs:element name="Email" type="Email" minOccurs="0" maxOccurs="1" nillable="false" />

                    <!-- Added by Param on 9/5/2013 -->
                    <xs:element name="BusinessName" minOccurs="0" maxOccurs="1" nillable="true">
                      <xs:simpleType>
                        <xs:restriction base="xs:string">
                          <xs:minLength value="0" />
                          <xs:maxLength value="50" />
                        </xs:restriction>
                      </xs:simpleType>
                    </xs:element>
                    <!-- Added by Param on 9/5/2013 -->
                    <xs:element name="BusinessType" type="BusinessEntityType" minOccurs="0" maxOccurs="1" nillable="false" />
                    <!-- Added by Param on 9/5/2013 -->
                    <xs:element name="BusinessTaxID" type="FederalTaxID" minOccurs="0" maxOccurs="1" nillable="false" />
                    <!-- Added by Param on 9/5/2013 -->
                    <xs:element name="BusinessPhone" type="Phone" minOccurs="0" maxOccurs="1" nillable="false" />
                    <!-- Added by Param on 9/5/2013 -->
                    <xs:element name="BusinessHowLongYears" type="xs:unsignedByte" minOccurs="0" maxOccurs="1" nillable="false" />
                    <!-- Added by Param on 9/5/2013 -->
                    <xs:element name="BusinessHowLongMonths" type="xs:unsignedByte" minOccurs="0" maxOccurs="1" nillable="false" />

                    <xs:element name="Addresses" minOccurs="0" maxOccurs="1">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="Address" minOccurs="0" maxOccurs="3">
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="IsCurrent" type="xs:boolean" minOccurs="1" maxOccurs="1" nillable="false" />
                                <!-- Added by Balaji on 3/2/2015 -->
                                <xs:element name="IsMailing" type="xs:boolean" minOccurs="1" maxOccurs="1" nillable="false" />
                                <!-- Modified by Param on 3/2/2015 to accomodate single line address for mailing address -->
                                <xs:element name="StreetNo" minOccurs="0" maxOccurs="1" nillable="true">
                                  <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                      <xs:minLength value="0" />
                                      <xs:maxLength value="20" />
                                    </xs:restriction>
                                  </xs:simpleType>
                                </xs:element>
                                <xs:element name="StreetName" minOccurs="1" maxOccurs="1" nillable="false">
                                  <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                      <xs:minLength value="1" />
                                      <!-- Modified by Param on 3/2/2015 to increase field length to accomodate single line address for mailing address -->
                                      <xs:maxLength value="100" />
                                    </xs:restriction>
                                  </xs:simpleType>
                                </xs:element>
                                <xs:element name="AptNo" minOccurs="0" maxOccurs="1" nillable="true">
                                  <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                      <xs:minLength value="0" />
                                      <xs:maxLength value="10" />
                                    </xs:restriction>
                                  </xs:simpleType>
                                </xs:element>
                                <xs:element name="City" type="City" minOccurs="1" maxOccurs="1" nillable="false" />
                                <xs:element name="County" type="County" minOccurs="0" maxOccurs="1" nillable="true" />
                                <xs:element name="State" type="State" minOccurs="1" maxOccurs="1" nillable="false" />
                                <xs:element name="ZipCode" type="ZipCode" minOccurs="1" maxOccurs="1" nillable="false" />
                                <!-- Modified by Param on 3/2/2015 to allow this field to be left out for mailing address -->
                                <xs:element name="Status" type="AddressStatus" minOccurs="0" maxOccurs="1" nillable="false" />
                                <xs:element name="HowLongYears" type="xs:unsignedByte" minOccurs="0" maxOccurs="1" nillable="false" />
                                <xs:element name="HowLongMonths" type="xs:unsignedByte" minOccurs="0" maxOccurs="1" nillable="false" />
                                <xs:element name="MonthlyPayment" type="xs:unsignedShort" minOccurs="0" maxOccurs="1" nillable="false" />
                                <xs:element name="LandLordMortgageCo" minOccurs="0" maxOccurs="1" nillable="true">
                                  <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                      <xs:minLength value="0" />
                                      <xs:maxLength value="30" />
                                    </xs:restriction>
                                  </xs:simpleType>
                                </xs:element>
                                <xs:element name="LandLordMortgagePhone" type="Phone" minOccurs="0" maxOccurs="1" nillable="false" />
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>

                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>

                    <xs:element name="EmploymentInfo" minOccurs="0" maxOccurs="1">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="Employment" minOccurs="0" maxOccurs="3">
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="IsCurrent" type="xs:boolean" minOccurs="1" maxOccurs="1" nillable="false" />
                                <xs:element name="NumOrder" type="xs:unsignedByte" minOccurs="1" maxOccurs="1" nillable="false" />
                                <xs:element name="Status" type="EmploymentStatus" minOccurs="1" maxOccurs="1" nillable="false" />
                                <xs:element name="Occupation" minOccurs="1" maxOccurs="1" nillable="false">
                                  <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                      <xs:minLength value="1" />
                                      <xs:maxLength value="50" />
                                    </xs:restriction>
                                  </xs:simpleType>
                                </xs:element>
                                <xs:element name="EmployerName" minOccurs="1" maxOccurs="1" nillable="false">
                                  <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                      <xs:minLength value="3" />
                                      <xs:maxLength value="50" />
                                    </xs:restriction>
                                  </xs:simpleType>
                                </xs:element>
                                <xs:element name="Address" minOccurs="0" maxOccurs="1" nillable="true">
                                  <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                      <xs:minLength value="0" />
                                      <xs:maxLength value="50" />
                                    </xs:restriction>
                                  </xs:simpleType>
                                </xs:element>
                                <xs:element name="City" type="City" minOccurs="0" maxOccurs="1" nillable="false" />
                                <xs:element name="State" type="State" minOccurs="0" maxOccurs="1" nillable="false" />
                                <xs:element name="ZipCode" type="ZipCode" minOccurs="0" maxOccurs="1" nillable="false" />
                                <!-- Modified by Param on 9/5/2013 -->
                                <xs:element name="GrossSalary" type="xs:unsignedInt" minOccurs="0" maxOccurs="1" nillable="false" />
                                <!-- Added by Param on 9/5/2013 -->
                                <xs:element name="GrossSalaryType" type="IncomeType" minOccurs="0" maxOccurs="1" nillable="false" />
                                <xs:element name="WorkPhone" type="Phone" minOccurs="0" maxOccurs="1" nillable="false" />
                                <xs:element name="HowLongYears" type="xs:unsignedByte" minOccurs="0" maxOccurs="1" nillable="false" />
                                <xs:element name="HowLongMonths" type="xs:unsignedByte" minOccurs="0" maxOccurs="1" nillable="false" />
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>

                    <xs:element name="OtherIncome" minOccurs="0" maxOccurs="1" nillable="false">
                      <xs:complexType>
                        <xs:sequence>
                          <!-- Modified by Param on 9/5/2013 -->
                          <xs:element name="Amount" type="xs:unsignedInt" minOccurs="1" maxOccurs="1" nillable="false" />
                          <!-- Added by Param on 9/5/2013 -->
                          <xs:element name="AmountType" type="IncomeType" minOccurs="1" maxOccurs="1" nillable="false" />
                          <xs:element name="Source" type="OtherIncomeSource" minOccurs="1" maxOccurs="1" nillable="false" />
                          <!-- Added by Balaji on 12/08/2016 for Applicant Other Income Source Description-->
                          <xs:element name="Description" minOccurs="0" maxOccurs="1" nillable="false">
                            <xs:simpleType>
                              <xs:restriction base="xs:string">
                                <xs:minLength value="0" />
                                <xs:maxLength value="100" />
                              </xs:restriction>
                            </xs:simpleType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>

                    <xs:element name="References" minOccurs="0" maxOccurs="1">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="Reference" minOccurs="0" maxOccurs="4">
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="Type" type="ReferenceType" minOccurs="1" maxOccurs="1" nillable="false" />
                                <xs:element name="NumOrder" type="xs:unsignedByte" minOccurs="1" maxOccurs="1" nillable="false" />
                                <xs:element name="Name" minOccurs="1" maxOccurs="1" nillable="false">
                                  <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                      <xs:minLength value="3" />
                                      <xs:maxLength value="50" />
                                    </xs:restriction>
                                  </xs:simpleType>
                                </xs:element>
                                <xs:element name="Checking" type="xs:boolean" minOccurs="0" maxOccurs="1" nillable="false" />
                                <xs:element name="Savings" type="xs:boolean" minOccurs="0" maxOccurs="1" nillable="false" />
                                <xs:element name="Address" minOccurs="0" maxOccurs="1" nillable="true">
                                  <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                      <xs:minLength value="0" />
                                      <xs:maxLength value="50" />
                                    </xs:restriction>
                                  </xs:simpleType>
                                </xs:element>
                                <xs:element name="City" type="City" minOccurs="0" maxOccurs="1" nillable="false" />
                                <xs:element name="State" type="State" minOccurs="0" maxOccurs="1" nillable="false" />
                                <xs:element name="ZipCode" type="ZipCode" minOccurs="0" maxOccurs="1" nillable="false" />
                                <xs:element name="Phone" type="Phone" minOccurs="0" maxOccurs="1" nillable="false" />
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>

                    <xs:element name="CreditBureaus" minOccurs="0" maxOccurs="1">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="CreditBureau" minOccurs="0" maxOccurs="3">
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="Source" type="CreditBureauSource" minOccurs="1" maxOccurs="1" nillable="false" />
                                <xs:element name="Score" type="xs:unsignedShort" minOccurs="1" maxOccurs="1" nillable="false" />
                                <xs:element name="ScoreRangeLow" type="xs:unsignedShort" minOccurs="0" maxOccurs="1" nillable="false" />
                                <xs:element name="ScoreRangeHigh" type="xs:unsignedShort" minOccurs="0" maxOccurs="1" nillable="false" />
                                <xs:element name="ScoreRankPercent" type="xs:unsignedByte" minOccurs="0" maxOccurs="1" nillable="false" />
                                <!-- Added by Param on 12/06/2011 -->
                                <!-- The base64 encoded representation of the raw XML report -->
                                <!-- NOTE: at this time only CREDCO XML format is supported -->
                                <xs:element name="XMLReport" type="xs:base64Binary" minOccurs="0" maxOccurs="1" nillable="false" />
                              </xs:sequence>
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>

                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>

        <xs:element name="Collateral" minOccurs="0" maxOccurs="1" nillable="false">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Type" type="CollateralType" minOccurs="1" maxOccurs="1" nillable="false" />

              <!-- added by Param on 6/5/2012 - Stock Number -->
              <xs:element name="StockNo" minOccurs="0" maxOccurs="1" nillable="true">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:minLength value="0" />
                    <xs:maxLength value="30" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>

              <xs:element name="Age" type="CollateralAge" minOccurs="1" maxOccurs="1" nillable="false" />
              <xs:element name="Serial" minOccurs="0" maxOccurs="1" nillable="true">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:minLength value="0" />
                    <xs:maxLength value="20" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="Year" type="xs:unsignedShort" minOccurs="0" maxOccurs="1" nillable="false" />
              <xs:element name="Make" minOccurs="0" maxOccurs="1" nillable="true">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:minLength value="0" />
                    <xs:maxLength value="30" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="Model" minOccurs="0" maxOccurs="1" nillable="true">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:minLength value="0" />
                    <xs:maxLength value="30" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>
              <xs:element name="Style" minOccurs="0" maxOccurs="1" nillable="true">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:minLength value="0" />
                    <xs:maxLength value="30" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>

              <!-- added by Param on 12/06/2011 - Color of the unit -->
              <xs:element name="Color" minOccurs="0" maxOccurs="1" nillable="true">
                <xs:simpleType>
                  <xs:restriction base="xs:string">
                    <xs:minLength value="0" />
                    <xs:maxLength value="30" />
                  </xs:restriction>
                </xs:simpleType>
              </xs:element>

              <xs:element name="Mileage" type="xs:unsignedInt" minOccurs="0" maxOccurs="1" nillable="false" />
              <xs:element name="Length" type="xs:unsignedByte" minOccurs="0" maxOccurs="1" nillable="false" />
              <xs:element name="FuelType" type="FuelType" minOccurs="0" maxOccurs="1" nillable="false" />
              <xs:element name="RVClass" type="RVClass" minOccurs="0" maxOccurs="1" nillable="false" />
              <xs:element name="MSRP" type="xs:decimal" minOccurs="0" maxOccurs="1" nillable="false" />
              <xs:element name="CostPrice" type="xs:decimal" minOccurs="0" maxOccurs="1" nillable="false" />
              <xs:element name="SellingPrice" type="xs:decimal" minOccurs="0" maxOccurs="1" nillable="false" />

              <!-- Added by Radhika on 7/6/2015 - -->
              <xs:element name="IsCPO" type="xs:boolean" minOccurs="1" maxOccurs="1" nillable="false" />
              
              <!-- Added by Balaji on 16/6/2016 - -->
              <xs:element name="EnableManualEntry" type="xs:boolean" minOccurs="0" maxOccurs="1" nillable="false" />
              
              <!-- Added by Balaji on 09/28/2016 - -->
              <xs:element name="Horsepower" type="xs:decimal" minOccurs="0" maxOccurs="1" nillable="false" />
              <xs:element name="Drive" type="DriveType" minOccurs="0" maxOccurs="1" nillable="false" />                   
              <!-- END by Balaji on 09/28/2016 - -->
              <xs:element name="Motors" minOccurs="0" maxOccurs="1">
                <xs:complexType>
                  <xs:sequence>
                    <!-- Modified by Balaji on 03/07/2016 - -->
                    <xs:element name="Motor" minOccurs="0" maxOccurs="3">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="NumOrder" type="xs:unsignedByte" minOccurs="1" maxOccurs="1" nillable="false" />
                          <xs:element name="Age" type="CollateralAge" minOccurs="0" maxOccurs="1" nillable="false" />
                          <xs:element name="Serial" minOccurs="0" maxOccurs="1" nillable="true">
                            <xs:simpleType>
                              <xs:restriction base="xs:string">
                                <xs:minLength value="0" />
                                <xs:maxLength value="20" />
                              </xs:restriction>
                            </xs:simpleType>
                          </xs:element>
